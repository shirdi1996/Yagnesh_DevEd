    <validationRules>
        <fullName>Address_Rule</fullName>
        <active>true</active>
        <description>Street City and  Country should either be blank or all filled up.</description>
        <errorConditionFormula>and(len(Street__c)=0, len(City__c)=0,len(Country__c)&gt;0) ||
and(len(Street__c)=0, len(City__c)&gt;0,len(Country__c)=0) ||
and(len(Street__c)=0, len(City__c)&gt;0,len(Country__c)&gt;0) ||
and(len(Street__c)&gt;0, len(City__c)=0,len(Country__c)=0) ||
and(len(Street__c)&gt;0, len(City__c)=0,len(Country__c)&gt;0) ||
and(len(Street__c)&gt;0, len(City__c)&gt;0,len(Country__c)=0)</errorConditionFormula>
        <errorDisplayField>Country__c</errorDisplayField>
        <errorMessage>Enter Complete address or leave it completely blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Account_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>len(Bank_Account_Number__c ) &gt;0  &amp;&amp; LEN(Bank_Account_Number__c )  &lt;&gt; 15</errorConditionFormula>
        <errorDisplayField>Bank_Account_Number__c</errorDisplayField>
        <errorMessage>Enter Proper Account Number--Account Number has to be 15 Digits exactly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Number_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>not(begins(Phone_Number__c, &quot;+&quot;))</errorConditionFormula>
        <errorDisplayField>Phone_Number__c</errorDisplayField>
        <errorMessage>Please enter the Country Code and &apos;+&apos; sign before the Country Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Price_edit_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Price__c )</errorConditionFormula>
        <errorDisplayField>Price__c</errorDisplayField>
        <errorMessage>Sorry!!! You cannot Edit the Price. Please contact your Manager for Permission</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Other_Rule</fullName>
        <active>true</active>
        <description>If &apos;Other&apos;  is selected for Reason then the Other Text Box should not left empty or null</description>
        <errorConditionFormula>ISPICKVAL( Reason__c ,&apos;Other&apos;) &amp;&amp;   ISBLANK (Other_Reason__c)</errorConditionFormula>
        <errorDisplayField>Other_Reason__c</errorDisplayField>
        <errorMessage>Since you selected Other for Reason please mention the reason, you cannot leave it blank.</errorMessage>
    </validationRules>
