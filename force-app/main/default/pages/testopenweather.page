              <apex:outputText label="Min_Temp" value="{!min_temp}" />
              <apex:outputText label="Max_Temp" value="{!max_temp}" />
              <apex:outputText label="SunRise" value="{!sunrise}" />
      </apex:pageBlockSection>
 </apex:pageBlock>>
</apex:page>